{
    "name": "hangulplus-syntax",
    "displayName": "한글+ 문법 강조",
    "description": "한글+ 프로그래밍 언어를 위한 문법 강조",
    "version": "0.0.1",
    "publisher": "siwoo",
    "engines": {
      "vscode": "^1.70.0"
    },
    "contributes": {
      "languages": [
        {
          "id": "hangulplus",
          "aliases": ["한글+"],
          "extensions": [".코드"],
          "configuration": "./language-configuration.json"
        }
      ],
      "grammars": [
        {
          "language": "hangulplus",
          "scopeName": "source.hangulplus",
          "path": "./syntaxes/한글+.tmLanguage.json"
        }
      ]
    }
  }
  